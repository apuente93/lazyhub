<% provide(:title, 'Contact') %>
<div class="sign_in_up_box">
<div class="row">
  <div class="span6 forms">
    <h3 class="sign_up2">Contact Us</h3>
    <%= form_for @contact, :html => {:class => 'form-horizontal' } do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :name, class: "labels" %>
      <%= f.text_field :name %>

      <%= f.label :email, class: "labels" %>
      <%= f.text_field :email %>

      <%= f.label :message, class: "labels" %>
      <%= f.text_area :message, :rows => "10" %>
      <%= f.submit "Send Message", class: "btn btn-md btn-primary center-block2" %>
    <% end %>
  </div>
</div>
</div>