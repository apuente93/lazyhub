<% provide(:title, "Sign in") %>
<h1 class="sign_up2">Sign in</h1>

<div class="row">
  <div class="span6 offset4">
    <%= form_for(:session, url: sessions_path) do |f| %>

      <%= f.label :email, class: "labels" %>
      <%= f.text_field :email %>

      <%= f.label :password, class: "labels"  %>
      <%= f.password_field :password  %>

      <%= f.submit "Sign in", class: "btn btn-large btn-primary center-block" %>
    <% end %>

    <p id="new_user_sign">New user? <%= link_to "Sign up now!", signup_path %></p>
  </div>
</div>